	<xs:complexType name="ProgramMembership">
		<xs:sequence>
			<xs:element name="ActionCode" type="xs:string" minOccurs="0"/>
			<xs:element name="EnteredBy" type="CodeTableDetail" minOccurs="0"/>
			<xs:element name="EnteredAt" type="CodeTableDetail" minOccurs="0"/>
			<xs:element name="EnteredOn" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="UpdatedOn" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ExternalId" type="xs:string" minOccurs="0"/>
			<xs:element name="EncounterNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="ProgramName" type="xs:string" minOccurs="0"/>
