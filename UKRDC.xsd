				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AdmissionSource">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AdmitReason">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HealthCareFacility">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail">
				<xs:sequence>
					<xs:element name="Organization" type="Organization" minOccurs="0"/>
					<xs:element name="LocationType" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="CLINIC"/>
								<xs:enumeration value="ER"/>
								<xs:enumeration value="DEPARTMENT"/>
								<xs:enumeration value="WARD"/>
								<xs:enumeration value="OTHER"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DischargeLocation">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SeparationMode">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Recommendation">
		<xs:sequence>
			<xs:element name="NoteText" type="xs:string" minOccurs="0"/>
			<xs:element name="RecipientPersons" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CareProvider" type="CareProvider" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RecipientOrganizations" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="HealthCareFacility" type="HealthCareFacility" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
