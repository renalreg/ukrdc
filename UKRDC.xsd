	</xs:complexType>
	<xs:complexType name="PersonalContactType">
		<xs:sequence>
			<xs:element name="Name"/>
			<xs:element name="ContactDetails" type="ContactDetail" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Relationship"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
