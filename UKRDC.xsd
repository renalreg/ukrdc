<xs:schema xmlns="http://www.rixg.org.uk/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.rixg.org.uk/">
	<xs:element name="PatientRecord">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SendingFacility">
					<xs:annotation>
						<xs:documentation>OCS Code of source trust (5-char e.g. RXF01)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="220"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Patient" type="Patient" minOccurs="1"/>
				<xs:element name="LabOrders" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LabOrder" type="LabOrder" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Encounters" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Encounter" type="Encounter" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Allergies" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Allergy" type="Allergy" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SocialHistories" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SocialHistory" type="SocialHistory" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Diagnoses" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Diagnosis" type="Diagnosis" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Observations" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Observation" type="Observation" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Procedures" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Procedure" type="Procedure" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Documents" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Document" type="Document" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Medications" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Medication" type="Medication" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ClinicalRelationships" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ClinicalRelationship" type="ClinicalRelationship" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProgramMemberships" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ProgramMembership" type="ProgramMembership" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Patient">
		<xs:sequence>
			<xs:element name="PatientNumbers" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PatientNumber" type="PatientNumber" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Names">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Name" type="Name" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>1 instance of name must be present if an NHS Number or CHI Number is included.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BirthTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>YYYYMMDD           Presume HHMM not required                            1 instance of name must be present if an NHS Number or CHI Number is included.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Gender" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From NHS Dictionary   0=Not Known             1=Male                2=Female                 9=Not Specified                  1 instance of name must be present if an NHS Number or CHI Number is included.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Addresses" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Address" type="Address" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ContactDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ContactDetail" type="ContactDetail" maxOccurs="4">
							<xs:annotation>
								<xs:documentation>Only 1 of each type should be sent, If Contact Details wrapper included then at least of of the Contact Detail elements must be present</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CountryOfBirth" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From NHS Data Dictionary   ISO 3166-1 Use the 3-char alphabetic code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FamilyDoctor" type="FamilyDoctor" minOccurs="0"/>
			<xs:element name="PersonToContact" type="PersonalContactType" minOccurs="0"/>
			<xs:element name="EthnicGroup" type="EthnicGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From NHS Data Dictionary   ETHNIC CATEGORY CODE which equates to RR18 (CodingStandard=ONS). From 2011 Scotland Census (CodingStandard=SCOT)   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Occupation" type="CodeTableDetail" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code range TBD </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PatientLanguage" type="CodeTableDetail" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From HL7 Table 0296 (??) TBC. Does this need to be a complex type?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Death" type="DeathType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Present only when dead - replaces isDead concept</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Name">
		<xs:sequence>
			<xs:element name="Prefix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Prefix or title e.g. Mr, Mrs etc</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Family" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Surname or family name. Mandatory on Usual name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Given" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation>Up to 2 given names. If the person has more than 2 concatenate in the 2nd occurrence. 1 occurrence is mandatory for a Usual name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Suffix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Suffix e.g. Jnr, Snr etc.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="use">
			<xs:annotation>
				<xs:documentation>Use codes from PDS		L=Usual			A=Alias 	(0..*)		PREFERRED=Preferred 		PREVIOUS-BIRTH=Birth name</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Language">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CodeTableDetail">
		<xs:sequence>
			<xs:element name="CodingStandard" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Code" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Description" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="User">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Organization">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail">
				<xs:sequence>
					<xs:element name="Address" type="Address" minOccurs="0"/>
					<xs:element name="ContactDetail" type="ContactDetail" minOccurs="0"/>
					<xs:element name="Trusts" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Trust" type="Trust" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Address">
		<xs:sequence>
			<xs:element name="FromTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ToTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Street">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Locality" minOccurs="0"/>
			<xs:element name="Town" type="xs:string"/>
			<xs:element name="County" type="xs:string" minOccurs="0"/>
			<xs:element name="Postcode" type="xs:string" minOccurs="0"/>
			<xs:element name="Country" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>From NHS Data Dictionary   ISO 3166-1 Use the 3-char alphabetic code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="use">
			<xs:annotation>
				<xs:documentation>From National MIM  H=Home      TMP=Temporary   PST=Postal</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ContactDetail">
		<xs:sequence>
			<xs:element name="Value" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The contact detail = phome number or email id</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Comments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>????</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="use">
			<xs:annotation>
				<xs:documentation>Based on HL7 table 201   PRN=Home Phone   WPN=Work Phone    PRS=Mobile Number    NET=Email </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Trust">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EthnicGroup">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PatientNumber">
		<xs:sequence>
			<xs:element name="Number" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Patient Idenfitication Number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CodeSystem" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code System defining the namespace of the number. NHS=NHS Number   CHI=CHI Number  RR=Renal Registry No. ODS:RXF=Trust Identifier where RXF is the ODS code HSC=Northern Ireland   - and others.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CareProvider">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail">
				<xs:sequence>
					<xs:element name="Name" type="Name" minOccurs="0"/>
					<xs:element name="Address" type="Address" minOccurs="0"/>
					<xs:element name="ContactDetail" type="ContactDetail" minOccurs="0"/>
					<xs:element name="CareProviderType" type="CareProviderType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CareProviderType">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FamilyDoctor">
		<xs:sequence>
			<xs:element name="GPPracticeId">
				<xs:annotation>
					<xs:documentation>National GP Practice Code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GPId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>National GP Code</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Encounter">
		<xs:sequence>
			<xs:element name="UpdatedOn" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="EncounterNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="AdmissionType" type="AdmissionType" minOccurs="0"/>
			<xs:element name="EncounterType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="E"/>
						<xs:enumeration value="I"/>
						<xs:enumeration value="O"/>
						<xs:enumeration value="N"/>
						<xs:enumeration value="G"/>
						<xs:enumeration value="P"/>
						<xs:enumeration value="S"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AdmittingClinician" type="CareProvider" minOccurs="0"/>
			<xs:element name="AttendingClinicians" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CareProvider" type="CareProvider" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ConsultingClinicians" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CareProvider" type="CareProvider" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ReferringClinician" type="ReferralDoctor" minOccurs="0"/>
			<xs:element name="AccountNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PriorVisitNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PreAdmissionNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AdmissionSource" type="AdmissionSource" minOccurs="0"/>
			<xs:element name="AssignedWard" type="xs:string" minOccurs="0"/>
			<xs:element name="AssignedRoom" type="xs:string" minOccurs="0"/>
			<xs:element name="AssignedBed" type="xs:string" minOccurs="0"/>
			<xs:element name="PriorWard" type="xs:string" minOccurs="0"/>
			<xs:element name="PriorRoom" type="xs:string" minOccurs="0"/>
			<xs:element name="PriorBed" type="xs:string" minOccurs="0"/>
			<xs:element name="AdmitReason" type="AdmitReason" minOccurs="0"/>
			<xs:element name="HealthCareFacility" type="HealthCareFacility" minOccurs="0"/>
			<xs:element name="DischargeLocation" type="DischargeLocation" minOccurs="0"/>
			<xs:element name="VisitDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="EncounterMRN" type="xs:string" minOccurs="0"/>
			<xs:element name="EndTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="SeparationMode" type="SeparationMode" minOccurs="0"/>
			<xs:element name="Specialties" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CareProvider" type="CareProvider" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RecommendationsProvided" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Recommendation" type="Recommendation" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExpectedAdmitTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ExpectedDischargeTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="PublicityCode" type="PublicityCode" minOccurs="0"/>
			<xs:element name="Priority" type="EncounterPriority" minOccurs="0"/>
			<xs:element name="ExpectedLOAReturnTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="DiagnosisRelatedGroup" type="DiagnosisRelatedGroup" minOccurs="0"/>
			<xs:element name="ActionCode" type="xs:string" minOccurs="0"/>
			<xs:element name="EnteredBy" type="User" minOccurs="0"/>
			<xs:element name="EnteredAt" type="Organization" minOccurs="0"/>
			<xs:element name="EnteredOn" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="FromTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ToTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ExternalId" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="220"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ActionScope" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdmissionType">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ReferralDoctor">
		<xs:complexContent>
			<xs:extension base="CodeTableDetail">
				<xs:sequence>
					<xs:element name="Name" type="Name" minOccurs="0"/>
